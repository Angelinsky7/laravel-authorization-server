{
    "name": "darkink/laravel-authorization-server",
    "description": "Darkink Authorization Server provides a way to serve authorization to an laravel passport auth",
    "keywords": ["laravel", "jwt", "oauth"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/angelinsky7/laravel-authorization-server/issues",
        "source": "https://github.com/angelinsky7/laravel-authorization-server"
    },
    "authors": [
        {
            "name": "Angelinsky7",
            "email": "steven.demarco@while-not-false.ch"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/auth": "^8.77",
        "illuminate/console": "^8.77",
        "illuminate/container": "^8.77",
        "illuminate/contracts": "^8.77",
        "illuminate/database": "^8.77",
        "illuminate/http": "^8.77",
        "illuminate/support": "^8.77",
        "lcobucci/jwt": "^4.1",
        "phpseclib/phpseclib": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Darkink\\AuthorizationServer\\": "src/",
            "Darkink\\AuthorizationServer\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Darkink\\AuthorizationServer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Darkink\\AuthorizationServer\\Providers\\PolicyServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
